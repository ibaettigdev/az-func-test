{
  "bindings": [
    {
      "name": "Timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "OutputBlob",
      "path": "nextlink/nextlink.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out",
      "type": "blob"
    },
    {
      "name": "InputBlob",
      "path": "nextlink/nextlink.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "type": "blob"
    }
  ]
}